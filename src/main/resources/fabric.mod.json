{
  "schemaVersion": 1,
  "id": "mobadditions",
  "version": "${version}",

  "name": "Mob Additions",
  "description": "Backporting some new mobs to Better than Wolves!",
  "authors": [
    "ITL Supress"
  ],
  "contact": {
    "homepage": "https://itlesports.com/",
    "sources": "https://github.com/ITLSupress/BTWMobAdditions/"
  },

  "license": "CC0-1.0",
  "icon": "mobadditions/icon.png",

  "environment": "*",
  "entrypoints": {
    "init": [
      "com.itlesports.mobadditions.MobAdditions",
      "com.itlesports.mobadditions.init.MobAdditionsLogger"
    ],
    "preLaunch": [
      "com.itlesports.mobadditions.init.PreLaunchInitializer"
    ]
  },
  "mixins": [
    "mobadditions.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4"
  },
  "suggests": {
    "flamingo": "*"
  }
}
